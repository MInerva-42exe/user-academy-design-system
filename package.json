{
  "name": "meua-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tokens": "style-dictionary build --config style-dictionary.config.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint:css": "stylelint \"src/**/*.css\" \"packages/**/*.css\"",
    "theme:sync": "mkdir -p packages/meua-theme/dist && cp -f src/theme/tokens.css packages/meua-theme/dist/tokens.css && cp -f src/theme/manageengine.css packages/meua-theme/dist/manageengine.css"
  },
  "dependencies": {
    "@carbon/react": "^1.91.0",
    "@carbon/styles": "^1.90.0",
    "@meua/theme": "workspace:*",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.33.0",
    "@storybook/addon-a11y": "^9.1.6",
    "@storybook/addon-docs": "^9.1.6",
    "@storybook/addon-vitest": "^9.1.6",
    "@storybook/react-vite": "^9.1.6",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.6",
    "globals": "^16.3.0",
    "playwright": "^1.55.0",
    "storybook": "^9.1.6",
    "style-dictionary": "^5.0.4",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-declaration-strict-value": "^1.10.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "workspaces": [
    "packages/*"
  ]
}